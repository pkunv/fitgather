generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Region {
  us
  eu
}

model Proxy {
  id     String @id @default(cuid())
  url    String
  region Region
}

model ApiKey {
  id     String @id @default(cuid())
  userId String
  secret String
}
