{
	"name": "fitgather",
	"version": "1.2.2",
	"private": true,
	"type": "module",
	"workspaces": [
		"services/spatula",
		"apps/www"
	],
	"scripts": {
		"www:build": "cd apps/www && next build",
		"www:db:generate": "cd apps/www && prisma migrate dev",
		"www:db:migrate": "cd apps/www && prisma migrate deploy",
		"www:db:push": "cd apps/www && prisma db push",
		"www:db:studio": "cd apps/www && prisma studio",
		"www:dev": "cd apps/www && next dev",
		"www:lint": "cd apps/www && next lint",
		"www:start": "cd apps/www && next start",
		"spatula:db:gen": "cd services/spatula && prisma generate",
		"spatula:db:push": "cd services/spatula && prisma db push",
		"postinstall": "cd apps/www && prisma generate && prisma db push && cd .. && cd services/spatula && prisma generate && primsa db push",
		"prepare": "husky",
		"test": "vitest",
		"test:e2e": "playwright test"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@playwright/test": "^1.46.0",
		"@types/eslint": "^8.56.10",
		"@types/node": "^20.14.10",
		"@types/react": "^18.3.3",
		"@types/react-dom": "^18.3.0",
		"@typescript-eslint/eslint-plugin": "^7.1.1",
		"@typescript-eslint/parser": "^7.1.1",
		"dotenv": "^16.4.5",
		"eslint": "^8.57.1",
		"husky": "^9.1.4",
		"jsdom": "^24.1.1",
		"prettier": "^3.3.2",
		"prettier-plugin-tailwindcss": "^0.6.5"
	},
	"ct3aMetadata": {
		"initVersion": "7.36.2"
	},
	"packageManager": "npm@10.1.0"
}
